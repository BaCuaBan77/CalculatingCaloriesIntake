<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.olole005.calculatingcaloriesintake">

    <application
        android:name=".LongLeApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AddNewFood"
            android:parentActivityName=".Opening"
            ></activity>
        <activity
            android:name=".ShowResult"
            android:parentActivityName=".Calculate" />
        <activity android:name=".Opening">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Calculate" android:parentActivityName=".Opening"></activity>
    </application>

</manifest>